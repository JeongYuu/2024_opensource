<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="@color/white"
    android:backgroundTintMode="src_over">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/mypage"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_weight="1"
                    android:background="#00FFFFFF"
                    android:gravity="start"
                    android:paddingLeft="12dp"
                    android:paddingTop="10dp"
                    android:paddingRight="12dp"
                    android:paddingBottom="0.1px"
                    android:text="👤"
                    android:textColor="#000000"
                    android:textSize="16sp"
                    app:iconSize="30dp"
                    app:rippleColor="#000000" />

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="80dp"
                    android:layout_weight="10"
                    app:srcCompat="@drawable/submate" />

                <View
                    android:id="@+id/view"
                    android:layout_width="50dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />
            </LinearLayout>

            <View
                android:id="@+id/view3"
                android:layout_width="match_parent"
                android:layout_height="4dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:background="#C3FEBF00" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/generalbutton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:backgroundTint="#FEBF00"
                    android:text="일반"
                    android:textSize="12dp"
                    android:minWidth="0dp"/>

                <Button
                    android:id="@+id/dietbutton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:autoSizeMaxTextSize="12dp"
                    android:autoSizeMinTextSize="8dp"
                    android:autoSizeStepGranularity="1sp"
                    android:autoSizeTextType="uniform"
                    android:backgroundTint="#548035"
                    android:textColor="#000000"
                    android:minWidth="0dp"
                    android:text="다이어트"
                    android:textSize="8dp" />

                <Button
                    android:id="@+id/premiumbutton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:backgroundTint="#FDBE00"
                    android:text="고급"
                    android:textSize="12dp"
                    android:minWidth="0dp"/>

                <Button
                    android:id="@+id/etcbutton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:backgroundTint="#548135"
                    android:text="기타"
                    android:textSize="12dp"
                    android:minWidth="0dp"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/diet_menu1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#00FEFEFE"
                android:clipToPadding="false"
                android:orientation="horizontal"
                android:paddingLeft="50px"
                android:paddingTop="40px"
                android:paddingRight="50px"
                android:paddingBottom="40px">

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="112dp"
                    android:layout_gravity="center"
                    android:clipToPadding="false"
                    android:foreground="@drawable/vege"
                    android:paddingLeft="30px"
                    android:paddingRight="30px"
                    app:cardCornerRadius="30dp"
                    app:cardUseCompatPadding="false"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent">

                </androidx.cardview.widget.CardView>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="베지"
                        android:textSize="20dp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="단품 4900원" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="세트 7500원" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="칼로리 209kcal" />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/diet_menu2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clipToPadding="false"
                android:orientation="horizontal"
                android:paddingLeft="50px"
                android:paddingTop="40px"
                android:paddingRight="50px"
                android:paddingBottom="40px">

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="112dp"
                    android:layout_gravity="center"
                    android:clipToPadding="false"
                    android:foreground="@drawable/shrimp"
                    android:paddingLeft="30px"
                    android:paddingRight="30px"
                    app:cardCornerRadius="30dp"
                    app:cardUseCompatPadding="false"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent">

                </androidx.cardview.widget.CardView>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="쉬림프"
                        android:textSize="20dp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="단품 7600원" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="세트 10200원" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="칼로리 241kcal" />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/diet_menu3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clipToPadding="false"
                android:orientation="horizontal"
                android:paddingLeft="50px"
                android:paddingTop="40px"
                android:paddingRight="50px"
                android:paddingBottom="40px">

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="112dp"
                    android:layout_gravity="center"
                    android:clipToPadding="false"
                    android:foreground="@drawable/r_chicken"
                    android:paddingLeft="30px"
                    android:paddingRight="30px"
                    app:cardCornerRadius="30dp"
                    app:cardUseCompatPadding="false"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent">

                </androidx.cardview.widget.CardView>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="로스트 치킨"
                        android:textSize="20dp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="단품 7300원" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="세트 9900원" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="칼로리 300kcal" />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/diet_menu4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clipToPadding="false"
                android:orientation="horizontal"
                android:paddingLeft="50px"
                android:paddingTop="40px"
                android:paddingRight="50px"
                android:paddingBottom="40px">

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="112dp"
                    android:layout_gravity="center"
                    android:clipToPadding="false"
                    android:foreground="@drawable/r_bbq"
                    android:paddingLeft="30px"
                    android:paddingRight="30px"
                    app:cardCornerRadius="30dp"
                    app:cardUseCompatPadding="false"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent">

                </androidx.cardview.widget.CardView>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="로티세리 바베큐"
                        android:textSize="20dp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="단품 7300원" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="세트 9900원" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="칼로리 327kcal" />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/diet_menu5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:clipToPadding="false"
                android:orientation="horizontal"
                android:paddingLeft="50px"
                android:paddingTop="40px"
                android:paddingRight="50px"
                android:paddingBottom="40px">

                <androidx.cardview.widget.CardView
                    android:layout_width="150dp"
                    android:layout_height="112dp"
                    android:layout_gravity="center"
                    android:clipToPadding="false"
                    android:foreground="@drawable/chicken_slice"
                    android:paddingLeft="30px"
                    android:paddingRight="30px"
                    app:cardCornerRadius="30dp"
                    app:cardUseCompatPadding="false"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent">

                </androidx.cardview.widget.CardView>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="치킨 슬라이스"
                        android:textSize="20dp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="단품 6500원" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="세트 9100원" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="칼로리 265kcal" />
                </LinearLayout>


            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>